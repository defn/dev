{
  "build": {
    "dockerfile": "Dockerfile"
  },
  "remoteUser": "ubuntu",
  "mounts": [
    "source=${localEnv:HOME}/.password-store,target=/home/ubuntu/.password-store,type=bind,consistency=delegated"
  ],
  "runArgs": [
    "-v",
    "/var/run/docker.sock:/var/run/docker.sock"
  ],
  "postStartCommand": "true"
}
