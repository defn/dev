{
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[python]": {
    "diffEditor.ignoreTrimWhitespace": false
  },
  "bazel.buildifierFixOnFormat": true,
  "breadcrumbs.enabled": false,
  "calva.paredit.defaultKeyMap": "strict",
  "calva.prettyPrintingOptions": {
    "enabled": true,
    "maxLength": 50,
    "printEngine": "pprint",
    "width": 120
  },
  "cue.formatTool": "cue fmt",
  "dev.containers.copyGitConfig": true,
  "dev.containers.gitCredentialHelperConfigLocation": "none",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.codeLens": false,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.lightbulb.enabled": "off",
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "editor.stickyScroll.enabled": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "Makefile": "makefile"
  },
  "files.autoSave": "afterDelay",
  "files.exclude": {
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*notifications/": true,
    "**/.trunk/*out/": true,
    "**/.trunk/*plugins/": true
  },
  "files.watcherExclude": {
    "**/.cache/**": true,
    "**/.git/**": true,
    "**/.kube/**": true,
    "**/.local/**": true,
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*notifications/": true,
    "**/.trunk/*out/": true,
    "**/.trunk/*plugins/": true,
    "**/node_modules/**": true,
    "**/work/**": true
  },
  "genieai.enableConversationHistory": true,
  "git.confirmSync": false,
  "git.detectSubmodules": false,
  "git.ignoreLimitWarning": true,
  "git.openRepositoryInParentFolders": "always",
  "git.suggestSmartCommit": false,
  "go.survey.prompt": false,
  "go.terminal.activateEnvironment": false,
  "go.toolsManagement.checkForUpdates": "off",
  "gopls": {
    "ui.semanticTokens": true
  },
  "markdown.marp.exportType": "html",
  "markdown.marp.html": "all",
  "npm.packageManager": "npm",
  "remote.autoForwardPortsSource": "hybrid",
  "remote.portsAttributes": {
    "default": {
      "onAutoForward": "ignore"
    }
  },
  "security.workspace.trust.enabled": true,
  "security.workspace.trust.startupPrompt": "never",
  "security.workspace.trust.trustedFolders": [
    "/home/ubuntu/m"
  ],
  "security.workspace.trust.untrustedFiles": "open",
  "svelte.enable-ts-plugin": true,
  "tailscale.portDiscovery.enabled": false,
  "tailscale.ssh.defaultUsername": "ubuntu",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.defaultProfile.linux": "dev",
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "terminal.integrated.env.linux": {
    "PATH": "/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin"
  },
  "terminal.integrated.fontFamily": "Cousine",
  "terminal.integrated.localEchoEnabled": "off",
  "terminal.integrated.profiles.linux": {
    "dev": {
      "args": [
        "--init-file",
        "~/.bash_entrypoint",
        "-s",
        "${workspaceFolder}"
      ],
      "path": "bash"
    }
  },
  "terminal.integrated.sendKeybindingsToShell": true,
  "terminal.integrated.showExitAlert": false,
  "terraform.languageServer.enable": false,
  "terraform.languageServer.ignoreSingleFileWarning": true,
  "trunk.showWhatsNew": false,
  "trunk.showWhatsNewMessage": false,
  "trunk.trunkGrayOutNonBlockingIssues": true,
  "workbench.colorTheme": "Default Dark Modern",
  "workbench.editor.empty.hint": "hidden",
  "workbench.startupEditor": "none",
  "terminal.integrated.stickyScroll.enabled": false,
  "remote.defaultExtensionsIfInstalledLocally": [],
  "chat.disableAIFeatures": true,
  "workbench.settings.showAISearchToggle": false,
  "files.trimTrailingWhitespace": true,
  "chat.commandCenter.enabled": false
}