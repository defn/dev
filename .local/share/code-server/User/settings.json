{
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "files.autoSave": "afterDelay",
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "extensions.ignoreRecommendations": true,
  "git.confirmSync": false,
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "terminal.integrated.localEchoEnabled": "off",
  "terminal.integrated.sendKeybindingsToShell": true,
  "terminal.integrated.showExitAlert": false,
  "terminal.integrated.defaultProfile.linux": "dev",
  "terminal.integrated.profiles.linux": {
    "dev": {
      "path": "bash",
      "args": ["--init-file", "~/.bash_entrypoint", "-s", "${workspaceFolder}"]
    }
  },
  "terminal.integrated.env.linux": {
    "PATH": "/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin"
  },
  "workbench.startupEditor": "none",
  "go.survey.prompt": false,
  "go.toolsManagement.checkForUpdates": "off",
  "gopls": {
    "ui.semanticTokens": true
  },
  "files.watcherExclude": {
    "**/.git/**": true,
    "**/node_modules/**": true,
    "**/.local/**": true,
    "**/.cache/**": true,
    "**/.kube/**": true,
    "**/work/**": true,
    "**/.trunk/*out/": true,
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*plugins/": true,
    "**/.trunk/*notifications/": true
  },
  "terraform.languageServer.ignoreSingleFileWarning": true,
  "terraform.languageServer.enable": false,
  "git.ignoreLimitWarning": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "git.openRepositoryInParentFolders": "always",
  "dev.containers.copyGitConfig": true,
  "dev.containers.gitCredentialHelperConfigLocation": "none",
  "go.terminal.activateEnvironment": false,
  "svelte.enable-ts-plugin": true,
  "cue.formatTool": "cue fmt",
  "trunk.trunkGrayOutNonBlockingIssues": true,
  "files.exclude": {
    "**/.trunk/*out/": true,
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*plugins/": true,
    "**/.trunk/*notifications/": true
  },
  "trunk.showWhatsNewMessage": false,
  "files.associations": {
    "Makefile": "makefile"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.suggestSmartCommit": false,
  "trunk.showWhatsNew": false,
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.colorTheme": "Default Dark Modern",
  "calva.prettyPrintingOptions": {
    "printEngine": "pprint",
    "enabled": true,
    "width": 120,
    "maxLength": 50
  },
  "calva.paredit.defaultKeyMap": "strict",
  "git.detectSubmodules": false,
  "workbench.editor.empty.hint": "hidden",
  "tailscale.portDiscovery.enabled": false,
  "tailscale.ssh.defaultUsername": "ubuntu",
  "[python]": {
    "diffEditor.ignoreTrimWhitespace": false
  },
  "remote.autoForwardPortsSource": "hybrid",
  "bazel.buildifierFixOnFormat": true,
  "bazel.enableCodeLens": true
}
