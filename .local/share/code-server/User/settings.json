{
    "files.autoSave": "afterDelay",
    "editor.formatOnSave": true,
    "editor.minimap.enabled": false,
    "extensions.ignoreRecommendations": true,
    "git.confirmSync": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.statusBar.enabled": false,
    "redhat.telemetry.enabled": false,
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "terminal.integrated.localEchoEnabled": "off",
    "terminal.integrated.sendKeybindingsToShell": true,
    "terminal.integrated.showExitAlert": false,
    "terminal.integrated.defaultProfile.linux": "dev",
    "terminal.integrated.profiles.linux": {
        "dev": {
            "path": "bash",
            "args": [
                "--init-file",
                "~/.bash_entrypoint",
                "-s",
                "${workspaceFolder}"
            ]
        },
    },
    "workbench.colorTheme": "Default Dark+",
    "workbench.editor.untitled.hint": "hidden",
    "workbench.startupEditor": "none",
    "calva.prettyPrintingOptions": {
        "printEngine": "pprint",
        "enabled": true,
        "width": 120,
        "maxLength": 50
    },
    "calva.paredit.defaultKeyMap": "strict",
    "go.survey.prompt": false,
    "go.toolsManagement.checkForUpdates": "off",
    "gopls": {
        "ui.semanticTokens": true
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "/home/ubuntu/.local/**": true,
        "/home/ubuntu/.cache/**": true,
        "/home/ubuntu/.kube/cache/**": true,
        "/home/ubuntu/work/**": true
    },
    "nix.formatterPath": "/tmp/.nix-profile/bin/nixpkgs-fmt",
    "terraform.languageServer.ignoreSingleFileWarning": true,
    "terraform.languageServer.enable": false,
    "git.ignoreLimitWarning": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "git.openRepositoryInParentFolders": "always",
    "go.terminal.activateEnvironment": false
}