    "autoprefixer": "^10.4.15",
    "eslint-config-custom": "workspace:*",
    "flowbite": "^1.8.1",
    "flowbite-svelte": "^0.43.2",
    "postcss": "^8.4.28",
    "postcss-load-config": "^4.0.1",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.0",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "ui": "workspace:*",
    "vite": "^4.4.9",
    "vitest": "^0.34.1"
