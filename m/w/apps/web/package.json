    "postcss": "^8.4.25",
    "postcss-load-config": "^4.0.1",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.5",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.3.2",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6",
    "ui": "workspace:*",
    "vite": "^4.4.2",
    "vitest": "^0.33.0"
