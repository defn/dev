{
  "//": {
    "metadata": {
      "backend": "s3",
      "stackName": "acc-spiral-hub",
      "version": "0.20.3"
    },
    "outputs": {}
  },
  "module": {
    "spiral-hub": {
      "//": {
        "metadata": {
          "path": "acc-spiral-hub/spiral-hub",
          "uniqueId": "spiral-hub"
        }
      }
    }
  },
  "terraform": {
    "backend": {
      "s3": {
        "bucket": "dfn-defn-terraform-state",
        "dynamodb_table": "dfn-defn-terraform-state-lock",
        "encrypt": true,
        "key": "stacks/acc-spiral-hub/terraform.tfstate",
        "profile": "defn-org-sso",
        "region": "us-east-1"
      }
    },
    "required_providers": {
      "aws": {
        "source": "aws",
        "version": "5.34.0"
      }
    }
  }
}
