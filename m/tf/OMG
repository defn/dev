terraform import aws_ssoadmin_permission_set.admin_sso_permission_set $(aws sso-admin list-permission-sets --instance-arn $(aws sso-admin list-instances --query 'Instances[0].InstanceArn' --output text | grep arn) | jq -r '.PermissionSets[]' | while read -r pid; do aws sso-admin describe-permission-set --instance-arn $(aws sso-admin list-instances --query 'Instances[0].InstanceArn' --output text | grep arn) --permission-set-arn $pid | jq -r '.PermissionSet | "\(.Name) \(.PermissionSetArn)"'; done | grep '^Administrator ' | awk '{print $2}'),$(aws sso-admin list-instances --query 'Instances[0].InstanceArn' --output text | grep arn)

terraform import $(terraform show .plan -no-color  | egrep '# aws_ssoadmin_managed|instance_arn|managed_policy_arn|permission_set_arn' | tail -4 | perl -pe 's{#}{}; s{will be.*}{}; s{\+ \S+\s+=}{}; s{"}{}g' | xargs | while read -r r sso role ps; do echo $r $role,$ps,$sso; done)
