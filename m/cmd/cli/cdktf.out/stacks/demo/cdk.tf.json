{
  "//": {
    "metadata": {
      "backend": "s3",
      "stackName": "demo",
      "version": "0.16.3"
    },
    "outputs": {
    }
  },
  "data": {
    "aws_ssoadmin_instances": {
      "sso_instance": {
        "//": {
          "metadata": {
            "path": "demo/sso_instance",
            "uniqueId": "sso_instance"
          }
        }
      }
    }
  },
  "locals": {
    "sso_instance_arn": "${data.aws_ssoadmin_instances.sso_instance.arns}",
    "sso_instance_isid": "${data.aws_ssoadmin_instances.sso_instance.identity_store_ids}"
  },
  "provider": {
    "aws": [
      {
        "profile": "demo",
        "region": "us-east-2"
      }
    ]
  },
  "resource": {
    "aws_identitystore_group": {
      "administrators_sso_group": {
        "//": {
          "metadata": {
            "path": "demo/administrators_sso_group",
            "uniqueId": "administrators_sso_group"
          }
        },
        "display_name": "Administrators",
        "identity_store_id": "${element(local.sso_instance_isid, 0)}"
      }
    },
    "aws_identitystore_group_membership": {
      "admin_sso_user_defn_membership": {
        "//": {
          "metadata": {
            "path": "demo/admin_sso_user_defn_membership",
            "uniqueId": "admin_sso_user_defn_membership"
          }
        },
        "group_id": "${aws_identitystore_group.administrators_sso_group.group_id}",
        "identity_store_id": "${element(local.sso_instance_isid, 0)}",
        "member_id": "${aws_identitystore_user.admin_sso_user_defn.user_id}"
      }
    },
    "aws_identitystore_user": {
      "admin_sso_user_defn": {
        "//": {
          "metadata": {
            "path": "demo/admin_sso_user_defn",
            "uniqueId": "admin_sso_user_defn"
          }
        },
        "display_name": "defn",
        "emails": {
          "primary": true,
          "type": "work",
          "value": "iam@defn.sh"
        },
        "identity_store_id": "${element(local.sso_instance_isid, 0)}",
        "name": {
          "family_name": "defn",
          "given_name": "defn"
        },
        "user_name": "defn"
      }
    },
    "aws_organizations_account": {
      "demo": {
        "//": {
          "metadata": {
            "path": "demo/demo",
            "uniqueId": "demo"
          }
        },
        "email": "aws-demo@defn.us",
        "name": "demo",
        "tags": {
          "ManagedBy": "Terraform"
        }
      },
      "ops": {
        "//": {
          "metadata": {
            "path": "demo/ops",
            "uniqueId": "ops"
          }
        },
        "email": "aws-demo+ops@defn.us",
        "iam_user_access_to_billing": "ALLOW",
        "name": "ops",
        "role_name": "OrganizationAccountAccessRole",
        "tags": {
          "ManagedBy": "Terraform"
        }
      }
    },
    "aws_organizations_organization": {
      "organization": {
        "//": {
          "metadata": {
            "path": "demo/organization",
            "uniqueId": "organization"
          }
        },
        "aws_service_access_principals": [
          "cloudtrail.amazonaws.com",
          "config.amazonaws.com",
          "ram.amazonaws.com",
          "ssm.amazonaws.com",
          "sso.amazonaws.com",
          "tagpolicies.tag.amazonaws.com"
        ],
        "enabled_policy_types": [
          "SERVICE_CONTROL_POLICY",
          "TAG_POLICY"
        ],
        "feature_set": "ALL"
      }
    },
    "aws_ssoadmin_account_assignment": {
      "demo_admin_sso_account_assignment": {
        "//": {
          "metadata": {
            "path": "demo/demo_admin_sso_account_assignment",
            "uniqueId": "demo_admin_sso_account_assignment"
          }
        },
        "instance_arn": "${aws_ssoadmin_managed_policy_attachment.admin_sso_managed_policy_attachment.instance_arn}",
        "permission_set_arn": "${aws_ssoadmin_managed_policy_attachment.admin_sso_managed_policy_attachment.permission_set_arn}",
        "principal_id": "${aws_identitystore_group.administrators_sso_group.group_id}",
        "principal_type": "GROUP",
        "target_id": "${aws_organizations_account.demo.id}",
        "target_type": "AWS_ACCOUNT"
      },
      "ops_admin_sso_account_assignment": {
        "//": {
          "metadata": {
            "path": "demo/ops_admin_sso_account_assignment",
            "uniqueId": "ops_admin_sso_account_assignment"
          }
        },
        "instance_arn": "${aws_ssoadmin_managed_policy_attachment.admin_sso_managed_policy_attachment.instance_arn}",
        "permission_set_arn": "${aws_ssoadmin_managed_policy_attachment.admin_sso_managed_policy_attachment.permission_set_arn}",
        "principal_id": "${aws_identitystore_group.administrators_sso_group.group_id}",
        "principal_type": "GROUP",
        "target_id": "${aws_organizations_account.ops.id}",
        "target_type": "AWS_ACCOUNT"
      }
    },
    "aws_ssoadmin_managed_policy_attachment": {
      "admin_sso_managed_policy_attachment": {
        "//": {
          "metadata": {
            "path": "demo/admin_sso_managed_policy_attachment",
            "uniqueId": "admin_sso_managed_policy_attachment"
          }
        },
        "instance_arn": "${aws_ssoadmin_permission_set.admin_sso_permission_set.instance_arn}",
        "managed_policy_arn": "arn:aws:iam::aws:policy/AdministratorAccess",
        "permission_set_arn": "${aws_ssoadmin_permission_set.admin_sso_permission_set.arn}"
      }
    },
    "aws_ssoadmin_permission_set": {
      "admin_sso_permission_set": {
        "//": {
          "metadata": {
            "path": "demo/admin_sso_permission_set",
            "uniqueId": "admin_sso_permission_set"
          }
        },
        "instance_arn": "${element(local.sso_instance_arn, 0)}",
        "name": "Administrator",
        "session_duration": "PT2H",
        "tags": {
          "ManagedBy": "Terraform"
        }
      }
    }
  },
  "terraform": {
    "backend": {
      "s3": {
        "bucket": "defn-bootstrap-remote-state",
        "dynamodb_table": "defn-bootstrap-remote-state",
        "encrypt": true,
        "key": "demo/terraform.tfstate",
        "profile": "terraform",
        "region": "us-east-2"
      }
    },
    "required_providers": {
      "aws": {
        "source": "aws",
        "version": "4.66.1"
      }
    }
  }
}