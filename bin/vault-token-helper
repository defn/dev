#!/usr/bin/env bash

export VAULT_ADDR="${VAULT_ADDR:-http://127.0.0.1:8200}"

function main {
    case "${1:-}" in
        get)
            cat /tmp/file-foo
            ;;
        store)
            true
            ;;
        erase)
            rm -f /tmp/file-foo
            ;;
        *)
            false
            ;;
    esac
}

main "$@"